# Hugging Face token for Pyannote models (get yours at https://huggingface.co/settings/tokens)
HF_TOKEN=hf_your_token_here

# Whisper model size: tiny, base, small, medium, large
WHISPER_MODEL=small

# Redis URL (default works with docker-compose)
REDIS_URL=redis://localhost:6379/0

# Jobs directory for temporary audio files
JOBS_DIR=./jobs

# Job TTL in seconds (default 24 hours)
JOB_TTL_SECONDS=86400
